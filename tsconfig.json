{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6"
    ],
    "types": ["reflect-metadata"],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "rootDir": "src",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "sourceMap": true
  }
}